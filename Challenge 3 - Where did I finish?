#include <bits/stdc++.h>
using namespace std;

int main() {
	// your code goes here
	
	int num;
	cin>>num;
	vector<int> a(num);
	for(int i=0;i<num;i++)
	cin>>a[i];
	
	int num2;
	cin>>num2;
	vector<int> b(num2);
	for(int i=0;i<num2;i++)
	cin>>b[i];
	
	unordered_map<int,int>m;
	sort(a.begin(),a.end(),[](int a,int b){return a>b;});
	int j=1;
	for(auto i:a)
	{
		if(!m[i])
		{
			m[i]=j;
		    j++;
		}
	}
	vector<int>res;
	bool f=false;
	for(int i=0;i<num2;i++)
	{
		f=false;
		for(int k=0;k<num;k++)
		{
			if(b[i]>=a[k])
			{
				f=true;
				res.push_back(m[a[k]]);
				break;
			}
		}
		if(f==false)
		res.push_back(m[a[num-1]]+1);
	}
	unordered_map<int,int>m1;
	for(auto i:res)
	m1[i]++;
	vector<pair<int,int>>v;
	
	for(auto i:m1)
	{
		v.push_back(make_pair(i.second,i.first));
	}
	sort(v.begin(),v.end(),[](auto i,auto k){if(i.first!=k.first) {return i.first>k.first;} else if(i.first==k.first){return i.second>k.second;}
	});
	cout<<v.begin()->second;
	return 0;
}
